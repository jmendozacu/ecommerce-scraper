    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box; 
    width: 170px;
    height: auto;   
}
.homeliving-header__submenu a:hover{
    background: #dfdfdf;
    color: #333;
    text-decoration: none;
}
.submenu__subcat__title {
    font-weight: bold;
    padding: 5px 12px;
    display: block;
}
.submenu__subcat .submenu__item {
    padding: 4px 12px 4px 17px;
}
.homeliving-header__submenu a span {
    display: inline-block;
    padding: 3px 0;
    line-height: auto;
}
.header__breadcrumb a:hover span {
    text-decoration: none;
}
.homeliving-header__menu__item:hover  .list-head:after { display: block;}
.homeliving-header__menu__item.active{ border-color: #ccc; background: #fff;}
.homeliving-header__menu__item.active:after { display: block;}
.homeliving-header__menu__item.active .homeliving-header__submenu { display: block; height: auto;}
.homeliving-header__menu__item.active .homeliving-header__submenu:after { display: block;}
.homeliving-header__menu__item.active:hover  .list-head:after { display: none;}

.submenu__item.more {
    font-weight: normal;
}
.submenu__item.more:hover {
    text-decoration: underline;
}
