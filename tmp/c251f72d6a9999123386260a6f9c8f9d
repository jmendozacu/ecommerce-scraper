$(".custlivechatbutton").click(function(e){
  e.preventDefault();
  var se = document.createElement('script'); se.type = 'text/javascript'; se.async = true;
  se.src = '//storage.googleapis.com/code.snapengage.com/js/72b6e844-1e7c-40fe-9b99-b7d4d1aa8d04.js';
  var done = false;
  se.onload = se.onreadystatechange = function() {
    if (!done&&(!this.readyState||this.readyState==='loaded'||this.readyState==='complete')) {
      done = true;
      SnapEngage.getAgentStatusAsync(function (online) {
        if(online) {
          $("#SnapABug_bImg").trigger("click");
        }else{
          alert("Mohon maaf, fasilitas livechat tersedia di jam kerja  09.00am - 09.00pm (Hari Kerja) dan 09.00am - 05.00pm (Hari Libur)")
          SnapEngage.hideButton();
        }
      });
    }
  };
  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(se, s);
});
</script>
