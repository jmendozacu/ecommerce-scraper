$(function(){
        

        String.prototype.formatMoneyExt = function(c, d, t){
            var n = this,
                c = isNaN(c = Math.abs(c)) ? 2 : c,
                d = d == undefined ? "." : d,
                t = t == undefined ? "," : t,
                s = n < 0 ? "-" : "",
                i = parseInt(n = Math.abs(+n || 0).toFixed(c)) + "",
                j = (j = i.length) > 3 ? j % 3 : 0;
            return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
        };
        var currencyFormat = {
            "ID": {
                format: "RP #",
                decimal: 2
            },
            "MY": {
                format: "RM #",
                decimal: 2
            },
            "PH": {
                format: "? #",
                decimal: 2
            },
            "TH": {
                format: "# ???",
                decimal: 2
            },
            "VN": {
                format: "# VND",
                decimal: 2
            },
            "SG": {
                format: "SGD #",
                decimal: 2
            }
        };

        var tmplPD = '',
            $productListHolder = $('#fashion__newin__product');

        var curFormatVenture = currencyFormat[store.venture];
        var serializeData = function (data){
            var arr_ = [];
            $.each(data, function(idx, obj){
                var ret = {
                    sku: '',
                    simpleSku: '',
                    name: '',
                    url: '',
                    brand: '',
                    price: '',
                    unformatPrice: '',
                    max_price: '',
                    specialPrice: false,
                    savingPercent: false,
                    rating: false,
                    image: false
                };

                if(typeof obj.data.simples !== 'undefined'){
                    $.each(obj.data.simples, function(key, value){
                        ret.simpleSku = key;
                    });
                    ret.sku = obj.data.sku;
                    ret.name = obj.data.name;
                    ret.url = obj.data.url.split('/mobapi')[1];
                    ret.brand = obj.data.brand;

                    ret.unformatPrice = obj.data.price;
                    ret.price = curFormatVenture.format.replace('#', obj.data.price.formatMoneyExt(curFormatVenture.decimal, '.', '.'));
                    ret.max_price = curFormatVenture.format.replace('#', obj.data.max_price.formatMoneyExt(curFormatVenture.decimal, '.', '.'));
                    if (typeof obj.data.special_price !== 'undefined') {
                        ret.specialPrice = curFormatVenture.format.replace('#', obj.data.special_price.formatMoneyExt(curFormatVenture.decimal, '.', '.'));

                        ret.unformatPrice = obj.data.special_price;
                        ret.price = curFormatVenture.format.replace('#', obj.data.special_price.formatMoneyExt(curFormatVenture.decimal, '.', '.'));
                        ret.max_price = curFormatVenture.format.replace('#', obj.data.price.formatMoneyExt(curFormatVenture.decimal, '.', '.'));
                        ret.savingPercent = obj.data.max_saving_percentage;
                    }

                    if(obj.images.length > 0){
                        ret.image = obj.images[0];
                    }

                    if(typeof obj.data.ratings_total.sum !== 'undefined' && obj.data.ratings_total.sum > 0){
                        ret.rating = {
                            sum: obj.data.ratings_total.sum,
                            avr: obj.data.ratings_total.avr,
                            avrPercent: (obj.data.ratings_total.avr / 5) * 100
                        }
                    }
                    arr_.push(ret);
                }
            });

            return arr_;
        };

        var getProduct = function(url, title){
            if (typeof title === 'undefined' || title == '') {
                title = false;
            }
            $.get(url, function(data){
                var showMoreFlag = false;
                if(data.success && typeof data.metadata.results !== 'undefined' && data.metadata.results.length > 0){
                    var preArray = data.metadata.results;
                    if (preArray.length >= 30) {
                        showMoreFlag = true;
                        preArray = data.metadata.results.slice(0,30);
                    }
                    var formatData = serializeData(preArray);
                    if(formatData.length > 0){
                        $productListHolder.html(Mustache.render(tmplPD, { data: formatData, title: title }));
                        //window.fbAsyncInit();
                    } else {
                        $productListHolder.html('no results');
                    }
                } else {
                    $productListHolder.html('no results');
                }

//                if (showMoreFlag) {
//                    var crossUrl = $calalogList.find('li.flashSaleListCatalogItem.red').data('cross-url');
//                    $('.showmoreBtn').attr('href', crossUrl).show();
//                } else {
//                    $('.showmoreBtn').hide();
//                }
            });
        }

tmplPD += '{{#data}}';
tmplPD += '<a href="http://www.lazada.co.id{{url}}" class="product-card fashion-theme__product-card new_false outofstock installments_">';
tmplPD += '    <div class="product-card__img">';
tmplPD += '        <span class="product-card__img-wrap">';
tmplPD += '            <img class="primary state_loaded" alt="{{name}}" title="{{name}}" height="240" width="160" src="{{#image}}{{path}}{{/image}}" style="display: inline;">';
tmplPD += '            <img class="secondary state_loaded" alt="{{name}}" title="{{name}}" height="240" width="160" src="{{#image}}{{path}}{{/image}}" style="display: block;">';
tmplPD += '        </span>';
tmplPD += '    </div>';
