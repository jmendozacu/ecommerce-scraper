<script type="text/javascript">
    var dataLayer = dataLayer || [];
    if (dataLayer !== undefined) {
        // check & validate hashtag if any
        var hashValue = window.location.hash ? window.location.hash.substr(1) : false;
        var regex = /^Q([^C]*)C(.*)$/;
        if (hashValue && regex.test(hashValue)) {
            var matches = hashValue.match(regex);
