function chromePatch() {$(".widget-banner_2x2, .widget-banner_1x1").css("transition", "0s")}
navigator.userAgent.search(/chrome/gi) > -1 && (chromePatch(), $('.sidebar__listItem').hover(chromePatch, chromePatch))

// hide category on dekstop
if (!document.getElementsByTagName('body')[0].classList.contains('mobile')) {
    // hide all link containt mbl-
    $('.catalog_navigator__content a[href*="mbl-"]').parent().parent().hide()
    $('.catalog_navigator__content a[href*="line-promo-"]').parent().parent().hide()
    // not containt mbl-
    var hideList = ['promo-tvc-mobile', 'moto-e-2014', 'mobile-weekend', 'beauty-blast', 'weekend-under-199rb', 'wow-wednesday', 'promo-xl-1111', 'line-timeline', 'line-timeline2', 'line-timeline3', 'line-chatpush', 'line-chatpush2-1', 'line-chatpush2-2', 'tsel20-karyawan', 'wechat-deals', 'line-chatlifestyle', 'line-chatmulticat', 'line-weekend', 'line-chat-fashion-women', 'line-chat-fashion-men', 'line-fashion', 'chat-el', 'timeline-el', 'line-bluelans', 'timeline-outdoor', 'chat-ls', 'timeline-ls', 'timeline-ha', 'line-bariho-timely', 'line-amayzingel', 'line-bestsellerls', 'line-watch1', 'tendencies-line', 'bestsellerfa', 'sis-kids-dealz', 'kebun-bibit-sis', 'opera-promo', 'ramadhan-electronics-sales-puasa5', 'ramadhan-electronics-sales-puasa10', 'line-jgos'];
    for(var hl in hideList){
        $(".catalog_navigator__list a[href*='"+ hideList[hl] +"']").parent().parent().hide()
    }
}
</script>
